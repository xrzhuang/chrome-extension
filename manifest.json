{
    "manifest_version": 2,
    "name": "Tim's Thoughts",
    "version": "0.1",
    "browser_action": {
        "default_icon": "img/logo.png"
    },

    "web_accessible_resources": [
        "*.jpg",
        "*.jpeg",
        "*.png",
        "*.json",
        "*.png/",
        "*.jpg/"
      ],

      "content_scripts": [
        {
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"],
        "css": ["style.css"]
        }
    ]
 }

